<div class="container">

    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="header">Mi contenido
            </h1>
        </div>
    </div>
    <!-- /.row -->
    <table class="table">
        <tr>
            <th>Titulo</th>
            <th>Privado</th>
            <th>Video</th>
            <th>Acciones</th>
        </tr>
      {{#each videos}}
          <tr>
              <td>{{this.title}}</td>
              <td>{{this.privateFile}}</td>
              <td><a href="{{this.video}}" target="_blank" class="btn">Ver video</a></td>
              <td>
                  <a href="/editar/{{this.id}}" class="btn">Editar</a>
              </td>
          </tr>
      {{/each}}
    </table>


</div>

